[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/AirUtah
[access "refs/for/refs/*"]
	read = group user/AirUtah
	push = group user/AirUtah
	label-Code-Review = -2..+2 group user/AirUtah
	label-Verified = -1..+1 group user/AirUtah
	rebase = group user/AirUtah
	removeReviewer = group user/AirUtah
	submit = group user/AirUtah
[access "refs/heads/*"]
	read = group user/AirUtah
	create = group user/AirUtah
	push = +force group user/AirUtah
	pushMerge = group user/AirUtah
[access "refs/tags/*"]
	push = +force group user/AirUtah
	pushTag = group user/AirUtah
	pushSignedTag = group user/AirUtah
